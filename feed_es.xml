---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>algorithmic Blog (es)</title>
		<description>Hago muchas cosas, aquí comparto mi <a href="https://rvalla.github.io/index_es">costado algorítmico</a>.</description>
		<link>{{ site.url }}</link>
		<atom:link href="{{ site.url }}/feed_es.xml" rel="self" type="application/rss+xml" />
		{% for post in site.posts limit:10 %}
			{% for tag in post.tags %}
				{% if tag == "spanish" %}
					<entry>
						<title>{{ post.title | xml_escape }}</title>
						<description>{{ post.excerpt | xml_escape }}</description>
						<pubDate>{{ post.date | date_to_string }}</pubDate>
						<link>{{ site.url }}{{ post.url }}</link>
						<content type="html">{{ post.content | xml_escape }}</content>
						<guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
					</entry>
				{% endif %}
			{% endfor %}
		{% endfor %}
	</channel>
</rss>